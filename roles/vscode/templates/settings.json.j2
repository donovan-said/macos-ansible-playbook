{
    "workbench.colorTheme": "Dracula",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    // Add "workbench.settings.editor": "json" to your settings.json file.
    // This will make it so that menu File → Preferences → Settings or ⌘ + ,
    // opens your settings.json file by default.
    "workbench.settings.editor": "json",
    // Font Settings
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 12,
    "editor.fontLigatures": true,
    // Tab Settingss
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    // Format Settings
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    // Ruler Settings
    "editor.rulers": [
        80,
        120
    ],
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1,
    "terminal.external.osxExec": "iTerm.app",
    "security.workspace.trust.untrustedFiles": "open",
    "settingsSync.ignoredSettings": [],
    "redhat.telemetry.enabled": true,
    "files.autoSave": "afterDelay",
    "python.venvPath": "/Users/donovansaid/.local/share/virtualenvs/",
    "[go][go.mod]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "golang.go",
        "editor.tabSize": 8
    },
    "go.delveConfig": {},
    "[python]": {
        "editor.formatOnSave": true
    },
    "[json]": {
        "editor.tabSize": 2
    },
    // MongoDB confirmation message
    "mdb.confirmRunAll": false,
    "go.toolsManagement.autoUpdate": true,
}